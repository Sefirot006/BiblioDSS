<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="DSSGenNHibernate"
		                   namespace="DSSGenNHibernate.EN.BibliotecaENIAC">
				
<joined-subclass name="PASEN" extends="DSSGenNHibernate.EN.BibliotecaENIAC.UsuarioEN" table="[PAS]">
	<key>
    		<column name="[DNIUsuario]"/>
   	</key>

			
		  			
						<bag name="Reserva" >
							<key>
								<column name="[FK_DNIUsuario_idPAS]"/>
					      	</key>
							<one-to-many class="DSSGenNHibernate.EN.BibliotecaENIAC.ReservaEN"/>
					    </bag>
					
			
		  			
						<many-to-one name="Director" class="DSSGenNHibernate.EN.BibliotecaENIAC.DirectorEN" >	
								<column name="[FK_DNIUsuario_idDirector]"/>
						</many-to-one>
											 					
					
					
		
		  </joined-subclass>	
		  
		  <query name="PASENReadAllHQL">from PASEN</query>
		</hibernate-mapping>
	